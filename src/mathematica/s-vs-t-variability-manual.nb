Cell["\<\
This shows a simple example. We can put all the parameters in one block for \
simplicity:\
\>", "Text",
 CellChangeTimes->{{3.9605260714851*^9, 3.960526080097417*^9}, {
  3.960526713451353*^9, 
  3.960526726545329*^9}},ExpressionUUID->"79f9b004-9345-4eec-8f88-\
9c10e2af138d"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"constant", " ", "values", " ", "for", " ", "now"}], " ", "*)"}], 
  "\n", 
  RowBox[{
   RowBox[{
    RowBox[{"epsF", " ", "=", " ", "1"}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"suitability", "-", 
     RowBox[{"function", " ", "constants"}]}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"suitParams", " ", "=", " ", 
     RowBox[{"<|", "\n", "  ", 
      RowBox[{
       RowBox[{"\"\<optT\>\"", "     ", "->", " ", "25"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"Optimal", " ", "temperature"}], " ", "*)"}], "\n", "  ", 
       RowBox[{"\"\<respBreadth\>\"", "  ", "->", " ", "150"}], ",", " ", 
       RowBox[{"(*", " ", 
        RowBox[{"Temperature", " ", "response", " ", "breadth"}], " ", "*)"}],
        "\n", "  ", 
       RowBox[{"\"\<Rhalf\>\"", "  ", "->", " ", "0.5"}], ",", "\n", "  ", 
       RowBox[{"\"\<mParams\>\"", "->", " ", 
        RowBox[{"{", 
         RowBox[{"0.05", ",", " ", "0.1", ",", " ", "0.05"}], "}"}]}]}], "\n",
       "|>"}]}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{"variable", " ", "things"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"nPatches", " ", "=", " ", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"totalS", " ", "=", " ", "1000"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"meanR", " ", "=", " ", "0.8"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"meanT", " ", "=", " ", "22"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"varR", " ", "=", " ", "0.2"}], ";"}], "\n", 
   RowBox[{
    RowBox[{"varT", " ", "=", " ", "2"}], ";"}], "\n", "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
    "make", " ", "a", " ", "landscape", " ", "to", " ", "start", " ", 
     "with"}], " ", "*)"}], "\n", 
   RowBox[{
    RowBox[{"temps", " ", "=", " ", 
     RowBox[{"RandomVariate", "[", 
      RowBox[{
       RowBox[{"NormalDistribution", "[", 
        RowBox[{"meanT", ",", " ", "varT"}], "]"}], ",", " ", "nPatches"}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"supp", "  ", "=", " ", 
     RowBox[{"RandomVariate", "[", 
      RowBox[{
       RowBox[{"NormalDistribution", "[", 
        RowBox[{"meanR", ",", " ", "varR"}], "]"}], ",", " ", "nPatches"}], 
      "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"wRaw", "  ", "=", " ", 
     RowBox[{"MapThread", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"SuitabilityFunc", "[", 
         RowBox[{"#1", ",", " ", "#2", ",", " ", "suitParams"}], "]"}], " ", 
        "&"}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"temps", ",", " ", "supp"}], "}"}]}], "]"}]}], ";"}], "\n", 
   RowBox[{
    RowBox[{"wNorm", " ", "=", " ", 
     RowBox[{"With", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"s", " ", "=", " ", 
         RowBox[{"Total", "[", "wRaw", "]"}]}], "}"}], ",", "\n", "  ", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{"s", " ", "==", " ", "0"}], ",", "\n", "     ", 
         RowBox[{"ConstantArray", "[", 
          RowBox[{"0", ",", " ", 
           RowBox[{"Length", "[", "wRaw", "]"}]}], "]"}], ",", "  ", 
         RowBox[{"(*", " ", 
          RowBox[{
          "all", " ", "zero", " ", "if", " ", "nothing", " ", "to", " ", 
           "normalize"}], " ", "*)"}], "\n", "     ", 
         RowBox[{"wRaw", "/", "s"}]}], "                          ", 
        RowBox[{"(*", " ", 
         RowBox[{
         "otherwise", " ", "the", " ", "usual", " ", "normalization"}], " ", 
         "*)"}], "\n", "  ", "]"}]}], "\n", "]"}]}], ";"}], "\n", 
   RowBox[{"(*", " ", 
    RowBox[{
     RowBox[{"this", " ", "is", " ", "the", " ", "landscape"}], "-", 
     RowBox[{"level", " ", "phi"}]}], " ", "*)"}], "\n", 
   RowBox[{"phi", " ", "=", " ", 
    RowBox[{"Total", "[", 
     RowBox[{"wNorm", " ", "*", " ", 
      RowBox[{"(", 
       RowBox[{"1", " ", "-", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", " ", "-", " ", 
           RowBox[{"(", 
            RowBox[{"wNorm", " ", "*", " ", "epsF"}], ")"}]}], ")"}], "^", 
         "totalS"}]}], ")"}]}], "]"}]}]}]}]], "Code",
 CellChangeTimes->{{3.960526729835936*^9, 3.9605267905842733`*^9}, {
  3.960531013146841*^9, 3.960531013537635*^9}, {3.960531067981341*^9, 
